{
    "name": "eonlab/large-file-upload",
    "description": "An extension of open-admin for uploading large files",
    "type": "library",
    "keywords": ["open-admin", "extension"],
    "homepage": "https://github.com/eonlab/large-file-upload",
    "license": "MIT",
    "authors": [
        {
            "name": "Dian Wang",
            "email": "dian126@foxmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "open-admin-org/open-admin": "~1.0",
        "peinhu/aetherupload-laravel": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "OpenAdmin\\LargeFileUpload\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OpenAdmin\\LargeFileUpload\\LargeFileUploadServiceProvider"
            ]

        }
    }
}
